<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Title>Moneybird SDK</Title>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Description>Moneybird SDK for .NET and .NET Core</Description>
        <Authors>Dutch and Bold</Authors>
        <LangVersion>latest</LangVersion>
        
        <PackageProjectUrl>https://github.com/dutch-and-bold/moneybird-sdk</PackageProjectUrl>
        <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
        <PackageIconUrl>https://github.com/dutch-and-bold/moneybird-sdk/raw/master/.github/moneybird-logo.png</PackageIconUrl>
        <RepositoryUrl>https://github.com/dutch-and-bold/moneybird-sdk</RepositoryUrl>

        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.0.0" />
      <PackageReference Include="System.Text.Json" Version="4.7.1" />
    </ItemGroup>

</Project>
